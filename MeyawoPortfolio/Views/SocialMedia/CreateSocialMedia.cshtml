
@{
    ViewBag.Title = "CreateSocialMedia";
    Layout = "~/Views/AdminLayout/_Layout.cshtml";
}

<h2>Sosyal Medya Ekle</h2>

<form method="post">

    <label>Sosyal Medya Başlığı</label>
    <input type="text" class="table table-bordered" name="Title" />
    <br />
    <label>Sosyal Medya Url</label>
    <input type="text" class="table table-bordered" name="SocialMediaUrl" />
    <br />
    <label>Sosyal Medya Icon</label>
    <input type="text" class="table table-bordered" name="Icon" />
    <br />

    <button class="btn btn-info">Sosyal Medya Kaydet</button>
</form>
